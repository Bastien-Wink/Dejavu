{
    "name": "Facebook feed cleaner",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Hide what have already been seen from Facebook's feed. Don't waste any more time passing seen stories.",
    "permissions": ["activeTab", "storage", "contextMenus"],
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Hidden count",
        "default_popup": "settings.html"
    },
    "content_scripts": [{
        "matches": ["https://www.facebook.com/"],
        "js": ["node_modules/jquery/dist/jquery.min.js", "node_modules/arrive/minified/arrive.min.js", "common.js", "wall.js"]
    }]
}
